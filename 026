class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int res=0,len=nums.size(),l=0;
        for(int i=0;i<len;){
            res++;
            nums[l++]=nums[i];
            do{
                i++;
            }while(i<len&&nums[i]==nums[i-1]);
        }
        return res;
    }
};
